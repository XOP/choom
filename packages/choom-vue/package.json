{
  "name": "choom-vue",
  "version": "0.1.0",
  "description": "UI building blocks for your next Vue 3.0 prototype",
  "keywords": [
    "vue",
    "vue 3",
    "ui",
    "design-system",
    "components",
    "presentational"
  ],
  "main": "lib/choom-vue.umd.js",
  "module": "lib/choom-vue.es.js",
  "exports": {
    ".": {
      "import": "./lib/choom-vue.es.js",
      "require": "./lib/choom-vue.umd.js"
    }
  },
  "files": [
    "lib/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "cleanup": "rm -rf lib && mkdir lib",
    "build": "yarn cleanup && vite build -c ./build/vite.config.js",
    "compile": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XOP/choom.git"
  },
  "author": "XOP",
  "license": "Apache-2.0",
  "homepage": "https://github.com/XOP/choom#readme",
  "dependencies": {
    "choom-theme": "0.1.0",
    "clsx": "^1.1.1",
    "nanoid": "^3.1.30",
    "vue-tabler-icons": "^2.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.1.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "sass": "^1.49.0",
    "vite": "^2.7.13",
    "vue": "^3.2"
  },
  "peerDependencies": {
    "vue": "^3.2"
  }
}
