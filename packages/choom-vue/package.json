{
  "name": "choom-vue",
  "version": "0.1.0",
  "description": "UI building blocks for your next Vue 3.0 prototype",
  "keywords": [
    "vue",
    "vue 3",
    "ui",
    "design-system",
    "components",
    "presentational"
  ],
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "files": [
    "lib/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "cleanup": "rm -rf lib && mkdir lib",
    "build:env": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:esm": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format esm",
    "build": "yarn cleanup && yarn build:env && yarn build:esm",
    "compile": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XOP/choom.git"
  },
  "author": "XOP",
  "license": "Apache-2.0",
  "homepage": "https://github.com/XOP/choom#readme",
  "dependencies": {
    "choom-theme": "0.1.0",
    "clsx": "^1.1.1",
    "nanoid": "^3.1.30",
    "vue-tabler-icons": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.11",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.2.10",
    "rollup": "^2.52.8",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.49.0",
    "vue": "^3.2"
  },
  "peerDependencies": {
    "vue": "^3.2"
  }
}
